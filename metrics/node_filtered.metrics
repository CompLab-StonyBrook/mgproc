# General format:
# name; LaTeX command; load_type; operator; trivial; filter; function
#
#
# tenure based, trivial memory load is ignored
#
MaxT ;    ; tenure; safemax; ; I, U, P, *
SumT ;    ; tenure; sum    ; ; I, U, P, *
BoxT ;    ; tenure; len    ; ; I, U, P, *
AvgT ;    ; tenure; avg    ; ; I, U, P, *
MaxTR;    ; tenure; sorted ; ; I, U, P, *
#
#
# tenure based, trivial memory load is not ignored
#
MaxT' ;   ; tenure; safemax; trivial; I, U, P, *
SumT' ;   ; tenure; sum    ; trivial; I, U, P, *
BoxT' ;   ; tenure; len    ; trivial; I, U, P, *
AvgT' ;   ; tenure; avg    ; trivial; I, U, P, *
MaxTR';   ; tenure; sorted ; trivial; I, U, P, *
#
#
# size based, trivial memory load is ignored
#
MaxS  ;   ; size  ; safemax;
SumS  ;   ; size  ; sum    ;
Movers;   ; size  ; len    ;
AvgS  ;   ; size  ; avg    ;
MaxTR ;   ; size  ; sorted ;
#
#
# size based, trivial memory load is not ignored
#
MaxT'  ;  ; size  ; safemax; trivial
SumT'  ;  ; size  ; sum    ; trivial
Movers';  ; size  ; len    ; trivial
AvgT'  ;  ; size  ; avg    ; trivial
MaxTR' ;  ; size  ; sorted ; trivial
